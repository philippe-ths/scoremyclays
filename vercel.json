{
  "buildCommand": "cd website && npm run build",
  "outputDirectory": "website/build",
  "installCommand": "cd website && npm install",
  "rewrites": [
    {
      "source": "/docs",
      "destination": "/index.html"
    },
    {
      "source": "/docs/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ]
}